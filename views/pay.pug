html
    head
        title DashBoard
        link(rel="stylesheet" type="text/css" href="/styles.css")
    body
        include ../views/header
        
        div#left(class = "column left")
        div#middle(class = "column middle")
            each member in data
                div.review
                    h6 Payment: 
                    h1 Name: #{member.user_name}
                    h1 Total Cost: $#{member.fee}
                    br
                    div.button1
                        button#remove(type = "button" onclick=`pay('${member.user_id}')`) Charge Payment
                    

        div#right(class = "column right")
           
        script(src="/pay.js")