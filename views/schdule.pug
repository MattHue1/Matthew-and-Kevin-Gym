html
    head
        title DashBoard
        link(rel="stylesheet" type="text/css" href="/styles.css")
    body
        include ../views/header
        
        div#left(class = "column left")
        div#middle(class = "column middle")
            h6 Add Classes:
            h1 Class Name: 
            |
            input#name(type='text', name='name')
            h1 Time: 
            |
            input#time(type='text', name='time')
            h1 Date: 
            |
            input#date(type='text', name='date')
            h1 Cost: 
            |
            input#fee(type='text', name='fee')
            h1 Room:
            select(id = 'room' name = 'room')
                each item in room
                    option(value = `${item.room_id}`) #{item.equipment_name}
            br
            div.button1
                button#add(type = "button" onclick=`add()`) Add Class
               
        div#right(class = "column right")
           
        script(src="/schdule.js")